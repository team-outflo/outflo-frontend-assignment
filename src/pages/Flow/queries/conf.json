          "type": "positive",
          "rule": {
            "type": "GROUP",
            "op": "AND",
            "children": [
              {
                "type": "RULE",
                "field": "connectionStatus",
                "cmp": "EQUAL",
                "value": "ACCEPTED"
              },
              {
                "type": "WAIT",
                "value": "WAIT",
                "metadata": {
                  "wait": 10
