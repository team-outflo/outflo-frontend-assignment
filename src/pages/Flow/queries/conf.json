    "CHECK_OPEN_PROFILE": {
      "type": "conditional",
      "label": "If Open Profile",
      "icon": "Lock",
      "conditions": [
        {
          "type": "positive",
          "label": "Yes",
          "rule": {
            "type": "GROUP",
            "op": "AND",
            "children": [
              {
                "type": "RULE",
                "field": "isProfileOpenedStatus",
                "cmp": "EQUAL",
                "value": "OPENED"
              }
            ]
          },
          "actionsAllowed": [
            "CHECK_CONNECTION",
            "SEND_CONNECTION_REQUEST",
            "SEND_MESSAGE",
            "SEND_INMAIL",
            "ENDORSE_SKILLS",
            "FOLLOW_PROFILE",
            "LIKE_A_POST",
            "VIEW_PROFILE"
          ]
        },
        {
          "type": "negative",
          "label": "No",
          "rule": {
            "type": "GROUP",
            "op": "AND",
            "children": [
              {
                "type": "RULE",
                "field": "isProfileOpenedStatus",
                "cmp": "EQUAL",
                "value": "NOT_OPENED"
              }
            ]
          },
          "icon": "NOT_OPENED_PROFILE",
          "actionsAllowed": [
            "CHECK_CONNECTION",
            "SEND_CONNECTION_REQUEST",
            "SEND_MESSAGE",
            "SEND_INMAIL",
            "ENDORSE_SKILLS",
            "FOLLOW_PROFILE",
            "LIKE_A_POST",
            "VIEW_PROFILE"
          ]
        }
      ]
